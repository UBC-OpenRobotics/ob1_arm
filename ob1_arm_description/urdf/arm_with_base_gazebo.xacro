                <baseline>0.2</baseline>
                <alwaysOn>true</alwaysOn>
                <updateRate>1.0</updateRate>
                <cameraName>placeholder_camera_ir</cameraName>
                <imageTopicName>/placeholder_camera/color/image_raw</imageTopicName>
